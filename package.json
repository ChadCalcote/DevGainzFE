{
    "name": "react-17-parcel-2-ts",
    "version": "1.0.0",
    "description": "react 17 and parcel 2 with typescript starter",
    "scripts": {
        "start": "parcel index.html",
        "build": "parcel build index.html",
        "check": "tsc --noEmit",
        "prepare": "husky install"
    },
    "dependencies": {
        "lodash": "4.17.10",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@types/lodash": "4.14.179",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "parcel": "2.4.1",
        "prettier": "2.7.1",
        "typescript": "4.4.4"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "keywords": [
        "parcel",
        "react",
        "typescript"
    ]
}
